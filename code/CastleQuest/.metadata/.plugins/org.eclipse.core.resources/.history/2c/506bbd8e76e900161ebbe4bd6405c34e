
public class Trap {

	
	//plagued rats, theif, caltrops, roadblock
	private String type;
	private String desc;
	
	public Trap(String t, String d){
		type = t;
		desc = d;
	}
	
	public String getType() {
		// TODO Auto-generated method stub
		return type;
	}
	public String getDesc(){
		return desc;
	}

}
