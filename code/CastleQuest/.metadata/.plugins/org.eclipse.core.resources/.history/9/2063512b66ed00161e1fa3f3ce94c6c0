/* Used to instantiate an enemy
* should use current space and player hp/cp to generate appropriate enemy
*/
public class Enemy{
	
	private int gold;
	private int CP;
	private int HP;
	
	
	public Enemy(int h, int c, int g){
		HP = h;
		CP = c;
		gold = g;
	}
	
	
	public boolean isDead(){
		return HP <= 0;
	}
	
	
	public void subtractHP(int h){
		HP -= h;
	}

	public int getGold() {
		return gold;
	}

	
}